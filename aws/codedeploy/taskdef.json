{
    "containerDefinitions": [{
        "name": "paidy-container",
        "image": "s3-to-rds-postgresql-pandas",
        "cpu": 0,
        "portMappings": [{
            "containerPort": 5000,
            "hostPort": 5000,
            "protocol": "tcp"
        }],
        "essential": true,
        "environment": [{
                "name": "DB_NAME",
                "value": "uscreditdb"
            },
            {
                "name": "DB_HOST",
                "value": "paidy-db-instance.ch6lnftk2ks9.us-east-1.rds.amazonaws.com"
            },
            {
                "name": "DB_PASS",
                "value": "123Admin"
            },
            {
                "name": "DB_USER",
                "value": "gnvml"
            }
        ],
        "environmentFiles": [
            {
                "value": "arn:aws:s3:::gnvml-us-credit/environment/.env",
                "type": "s3"
            }
        ],
        "mountPoints": [],
        "volumesFrom": [],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/paidy-task-def",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }],
    "family": "paidy-task-def",
    "executionRoleArn": "arn:aws:iam::456668253361:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresAttributes": [{
            "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
        },
        {
            "name": "ecs.capability.execution-role-awslogs"
        },
        {
            "name": "com.amazonaws.ecs.capability.ecr-auth"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
        },
        {
            "name": "ecs.capability.execution-role-ecr-pull"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
        },
        {
            "name": "ecs.capability.task-eni"
        }
    ],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}